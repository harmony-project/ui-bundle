{
  "name": "harmony-project/ui-bundle",
  "type": "symfony-bundle",
  "description": "Integrates Semantic UI 2 into the Symfony framework",
  "keywords": ["symfony", "semantic-ui"],
  "homepage": "http://harmony-project.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Goudriaan",
      "homepage": "http://codedmonkey.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/http-kernel": "~3.0|~4.0"
